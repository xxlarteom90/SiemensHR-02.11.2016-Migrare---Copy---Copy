        </Field>
        <Field Name="NrTichete">
          <DataField>NrTichete</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SiemensHR_Test</DataSourceName>
        <CommandText>SELECT     v_StatDePlata.Marca, v_StatDePlata.Nume, v_StatDePlata.Prenume, Angajati.ProgramLucru AS Norma, 
                      v_StatDePlata.NrZileLucrateLuna - v_StatDePlata.NrOreTotalDelegatieInterna - v_StatDePlata.NrOreTotalDelegatieExterna AS NrTichete
FROM         v_StatDePlata INNER JOIN
                      Angajatori ON v_StatDePlata.AngajatorID = Angajatori.AngajatorID INNER JOIN
                      Sal_Luni ON v_StatDePlata.LunaID = Sal_Luni.LunaID INNER JOIN
                      Departamente ON v_StatDePlata.DepartamentID = Departamente.DepartamentID INNER JOIN
                      Angajati ON v_StatDePlata.AngajatID = Angajati.AngajatID INNER JOIN
                      Departamente Departamente_1 ON Departamente.DeptParinte = Departamente_1.DepartamentID
WHERE     (v_StatDePlata.LunaID = @LunaID) AND (v_StatDePlata.AngajatorID = @AngajatorID) AND (Departamente.DeptParinte = @DepartamentID) AND 
                      (Angajati.ModIncadrare = 0) OR
                      (v_StatDePlata.LunaID = @LunaID) AND (v_StatDePlata.AngajatorID = @AngajatorID) AND (Angajati.ModIncadrare = 0) AND 
                      (Departamente.DepartamentID = @DepartamentID)
ORDER BY v_StatDePlata.Nume, v_StatDePlata.Prenume</CommandText>
        <QueryParameters>
          <QueryParameter Name="@LunaID">
            <Value>=Parameters!LunaID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepartamentID">
            <Value>=Parameters!DepartamentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="dsDepartament">
      <Fields>
        <Field Name="Departament">
          <DataField>Departament</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SiemensHR_Test</DataSourceName>
        <CommandText>SELECT     Denumire AS Departament
FROM         Departamente
WHERE     (DepartamentID = @DepartamentID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DepartamentID">
            <Value>=Parameters!DepartamentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
