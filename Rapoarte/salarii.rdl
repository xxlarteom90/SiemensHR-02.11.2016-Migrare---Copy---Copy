        <Field Name="SalariuAngajat">
          <DataField>SalariuAngajat</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DateAngajator</DataSourceName>
        <CommandText>SELECT     Angajati.NumeIntreg AS NumeAngajat, Banci.CodBanca AS CodBancaAngajat, ConturiAngajati.NumarCont AS NumarContAngajat, 
                      sal_StatDePlata.RestDePlata AS SalariuAngajat
FROM         Angajati INNER JOIN
                      ConturiAngajati ON Angajati.AngajatID = ConturiAngajati.AngajatID INNER JOIN
                      Banci ON ConturiAngajati.BancaID = Banci.BancaID INNER JOIN
                      sal_StatDePlata ON Angajati.AngajatID = sal_StatDePlata.AngajatID
WHERE     (Angajati.AngajatorID = @AngajatorID) AND (ConturiAngajati.Activ = 1) AND (ConturiAngajati.Moneda = @Moneda) AND 
                      (sal_StatDePlata.LunaID = @LunaID)
ORDER BY Angajati.NumeIntreg</CommandText>
        <QueryParameters>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Moneda">
            <Value>=Parameters!Moneda.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LunaID">
            <Value>=Parameters!LunaID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DateAngajator">
      <Fields>
        <Field Name="DenumireAngajator">
