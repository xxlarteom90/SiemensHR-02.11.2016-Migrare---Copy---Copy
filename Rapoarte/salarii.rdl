        </Field>
        <Field Name="NumarContVechiAngajator">
          <DataField>NumarContVechiAngajator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumarContIBANAngajator">
          <DataField>NumarContIBANAngajator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DateAngajator</DataSourceName>
        <CommandText>SELECT     Angajatori.Denumire AS DenumireAngajator, Angajatori.CUI_CNP, Banci.CodBanca AS CodBancaAngajator, 
                      Conturi.NumarContVechi AS NumarContVechiAngajator, Conturi.NumarContIBAN AS NumarContIBANAngajator
FROM         Banci INNER JOIN
                      Conturi ON Banci.BancaID = Conturi.BancaID INNER JOIN
                      Angajatori ON Conturi.AngajatorID = Angajatori.AngajatorID
WHERE     (Conturi.ContID = @ContID) AND (Angajatori.AngajatorID = @AngajatorID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ContID">
            <Value>=Parameters!ContID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
