      <Query>
        <DataSourceName>SiemensHR_Test</DataSourceName>
        <CommandText>SELECT     AngajatFull.Marca, AngajatFull.NumeIntreg, Traininguri.Nume AS DenumireCurs, Traininguri.Descriere AS DescriereCurs, 
                      IstoricTraining.DataStart AS DataInceput, IstoricTraining.DataEnd AS DataSfarsit, AngajatFull.DepartamentDenumire
FROM         AngajatFull INNER JOIN
                      IstoricTraining ON AngajatFull.AngajatID = IstoricTraining.AngajatID INNER JOIN
                      Traininguri ON IstoricTraining.TrainingID = Traininguri.TrainingID LEFT OUTER JOIN
                      Lichidare ON AngajatFull.AngajatID = Lichidare.AngajatID CROSS JOIN
                      Sal_Luni
WHERE     (Sal_Luni.LunaID = @LunaID) AND (AngajatFull.AngajatorID = @AngajatorID) AND (AngajatFull.Lichidat = 0) OR
                      (Sal_Luni.LunaID = @LunaID) AND (AngajatFull.AngajatorID = @AngajatorID) AND (DATEDIFF(mm, Lichidare.DataLichidare, Sal_Luni.Data) = 0)
ORDER BY AngajatFull.NumeIntreg, IstoricTraining.DataStart DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@LunaID">
            <Value>=Parameters!LunaID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
