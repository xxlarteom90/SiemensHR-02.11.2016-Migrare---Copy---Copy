      <Query>
        <DataSourceName>SiemensHR</DataSourceName>
        <CommandText>SELECT     AngajatFull.Marca, AngajatFull.Nume, AngajatFull.Prenume, CentreCost.Nume AS CentruCost, CentreCost.Cod AS CodCentruCost
FROM         AngajatFull INNER JOIN
                      Departamente ON AngajatFull.DepartamentID = Departamente.DepartamentID INNER JOIN
                      CentreCost ON Departamente.CentruCostID = CentreCost.CentruCostID
WHERE     (AngajatFull.AngajatorID = @AngajatorID)
ORDER BY AngajatFull.Nume, AngajatFull.Prenume, AngajatFull.CentruCostNume</CommandText>
        <QueryParameters>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
