      <Query>
        <DataSourceName>SiemensHR_Test</DataSourceName>
        <CommandText>SELECT     AngajatFull.Marca, AngajatFull.Nume, AngajatFull.Prenume, 
                      Sal_SituatieLunaraAngajati.NrZileLucrateLuna - Sal_SituatieLunaraAngajati.NrOreTotalDelegatieInterna - Sal_SituatieLunaraAngajati.NrOreTotalDelegatieExterna
                       AS NrTichete, (CASE WHEN AngajatFull.CNP IS NOT NULL THEN CAST(AngajatFull.CNP AS varchar(13)) ELSE CAST(AngajatFull.NIF AS varchar(13)) 
                      END) AS CNP, AngajatFull.ProgramLucru AS Norma
FROM         Sal_SituatieLunaraAngajati INNER JOIN
                      Sal_Luni ON Sal_SituatieLunaraAngajati.LunaID = Sal_Luni.LunaID INNER JOIN
                      AngajatFull ON Sal_SituatieLunaraAngajati.AngajatID = AngajatFull.AngajatID INNER JOIN
                      Angajatori ON AngajatFull.AngajatorID = Angajatori.AngajatorID INNER JOIN
                      Departamente ON AngajatFull.DepartamentID = Departamente.DepartamentID INNER JOIN
                      Departamente Departamente_1 ON Departamente.DeptParinte = Departamente_1.DepartamentID INNER JOIN
                      sal_StatDePlata ON Sal_Luni.LunaID = sal_StatDePlata.LunaID AND Sal_SituatieLunaraAngajati.AngajatID = sal_StatDePlata.AngajatID
WHERE     (Sal_SituatieLunaraAngajati.LunaID = @LunaID) AND (Departamente.DeptParinte = @DepartamentID) AND (AngajatFull.AngajatorID = @AngajatorID) AND
                       (AngajatFull.ModIncadrare = 0) OR
                      (Sal_SituatieLunaraAngajati.LunaID = @LunaID) AND (Departamente.DepartamentID = @DepartamentID) AND (AngajatFull.AngajatorID = @AngajatorID) 
                      AND (AngajatFull.ModIncadrare = 0)
ORDER BY AngajatFull.Nume, AngajatFull.Prenume</CommandText>
        <QueryParameters>
          <QueryParameter Name="@LunaID">
            <Value>=Parameters!LunaID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepartamentID">
            <Value>=Parameters!DepartamentID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AngajatorID">
            <Value>=Parameters!AngajatorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="dsDepartament">
      <Fields>
        <Field Name="Departament">
          <DataField>Departament</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SiemensHR_Test</DataSourceName>
        <CommandText>SELECT     Denumire AS Departament
FROM         Departamente
WHERE     (DepartamentID = @DepartamentID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DepartamentID">
            <Value>=Parameters!DepartamentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
